# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
Host devnexus
    HostName 192.168.1.100
    User jonas
    IdentityFile C:\Users\Jonas\.ssh\id_rsa

Host devroot
    HostName 192.168.1.100
    User root
    IdentityFile C:\Users\Jonas\.ssh\id_rsa

Host sitehost
    HostName ssh.sitehost.iu.edu
    User jonmcclu
    IdentityFile C:\Users\Jonas\.ssh\id_webtech

Host sitehost-test
    HostName ssh.sitehost-test.iu.edu
    User jonmcclu
    IdentityFile C:\Users\Jonas\.ssh\id_webtech

Host pages
    HostName ssh-pages.iu.edu
    User jonmcclu
    IdentityFile C:\Users\Jonas\.ssh\id_webtech


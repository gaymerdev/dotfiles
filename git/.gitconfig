[init]
	defaultBranch = main
[core]
	editor = vim
[color]
	ui = true
[commit]
	template = ~/.gitmessage.txt
[url "https://github.com"]
	insteadOf = gh:
[url "https://gitlab.com"]
	insteadOf = gl:
[user]
	name = Jonas McClure
	email = jonmcclu@proton.me
	signingkey = ~/.ssh/id_ed25519
[gpg]
	format = ssh
[commit]
	gpgsign = true
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
